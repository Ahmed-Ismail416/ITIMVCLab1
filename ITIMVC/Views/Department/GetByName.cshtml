@model ITIMVC.Models.Department

<h2>Department Search Result</h2>

@if (Model != null)
{
    <ul>
        <li><strong>ID:</strong> @Model.DeptId</li>
        <li><strong>Name:</strong> @Model.Name</li>
        <li><strong>Manager:</strong> @Model.Manager</li>
        <li><strong>Location:</strong> @Model.Location</li>
        <li><strong>Branches:</strong> @Model.Branches</li>
    </ul>
}
else
{
    <p>No department found with that name.</p>
}
