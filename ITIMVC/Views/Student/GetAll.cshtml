@model List<ITIMVC.Models.Student>

<h2>All Students</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>SSN</th>
            <th>Name</th>
            <th>Age</th>
            <th>Address</th>
            <th>Image</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.ssn</td>
                <td>@s.name</td>
                <td>@s.age</td>
                <td>@s.address</td>
                <td><img src="@s.image" width="80" /></td>
                <td>@s.email</td>
            </tr>
        }
    </tbody>
</table>
