@model List<ITIMVC.Models.Student>

<h2>All Students</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>SSN</th>
            <th>Name</th>
            <th>Age</th>
            <th>Address</th>
            <th>Image</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var s in Model)
        {
            <tr>
                <td>@s.SSN</td>
                <td>@s.Name</td>
                <td>@s.Age</td>
                <td>@s.Address</td>
                <td><img src="@s.Image" width="80" /></td>
                <td>@s.Email</td>
            </tr>
        }
    </tbody>
</table>
